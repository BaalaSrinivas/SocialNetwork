{
  "Routes": [
    {
      "DownstreamPathTemplate": "/contentapi/{version}/content/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "5002"
        }
      ],
      "UpstreamPathTemplate": "/api/{version}/content/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST" ]
    },
    {
      "DownstreamPathTemplate": "/userapi/{version}/user",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": "5007"
        }
      ],
      "UpstreamPathTemplate": "/api/{version}/user",
      "UpstreamHttpMethod": [ "GET", "POST","PUT" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5000"
  }
}